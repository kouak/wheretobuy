<% title "Store" %>

<p>
  <strong>Name:</strong>
  <%=h @store.name %>
</p>
<p>
  <strong>Url:</strong>
  <%=h @store.url %>
</p>
<p>
  <strong>Address:</strong>
  <%=h @store.address %>
</p>
<p>
  <strong>City:</strong>
  <%=h @store.city_id %>
</p>
<p>
  <strong>Online Shop:</strong>
  <%=h @store.online_shop %>
</p>

<p>
  <%= link_to "Edit", edit_store_path(@store) %> |
  <%= link_to "Destroy", @store, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", stores_path %>
</p>
